  <title>MyTribe</title>
  <link rel="icon" href="/favicon.ico">
  <link rel="stylesheet" href="https://static.winnie.com/css/bootstrap.min.css">
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Lato:300,400,400i,700,700i">
